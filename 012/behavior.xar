    def __init__(self):
        GeneratedClass.__init__(self)

    def onLoad(self):
        self.memory = ALProxy("ALMemory")
        self.isRuning = False

    def onUnload(self):
        #put clean-up code here
        pass

    def onInput_onStart(self):
        if not self.isRuning:
            import socket

            response = ""
            try:
                host = self.memory.getData("ComSoftbankrobotics/SampleProject/Setting/iremocon/ip")
                port = int(self.memory.getData("ComSoftbankrobotics/SampleProject/Setting/iremocon/port"))

                client = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
                client.connect((host, port))
                client.settimeout(20)
                client.send("*is;%s\r\n" % self.getParameter("key"))
                response = client.recv(4096)

                client.close()
            except:
                pass

            if "is;ok" in str(response):
                self.onStopped()
            else:
                self.memory.raiseEvent("ComSoftbankrobotics/SampleProject/Error", 200)


