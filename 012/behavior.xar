            import socket

            response = ""
            try:
                host = self.memory.getData("ComSoftbankrobotics/SampleProject/Setting/iremocon/ip")
                port = int(self.memory.getData("ComSoftbankrobotics/SampleProject/Setting/iremocon/port"))

                client = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
